{
    "name": "symfony/ai-mate",
    "description": "AI development assistant MCP server for Symfony projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Johannes Wachter",
            "email": "johannes@sulu.io"
        },
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "bin": [
        "bin/mate"
    ],
    "require": {
        "php": "^8.1",
        "mcp/sdk": "^0.1.0",
        "psr/log": "^2.0|^3.0",
        "symfony/config": "^6.4 || ^7.3 || ^8.0",
        "symfony/console": "^5.4|^6.4|^7.4|^8.0",
        "symfony/dependency-injection": "^6.4 || ^7.3 || ^8.0",
        "symfony/finder": "^6.4 || ^7.3 || ^8.0"
    },
    "require-dev": {
        "ext-simplexml": "*",
        "php-cs-fixer/shim": "^3.75",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.5",
        "symfony/dotenv": "^5.4|^6.4|^7.4|^8.0"
    },
    "conflict": {
        "symfony/dotenv": "<5.4.10"
    },
    "scripts": {
        "phpstan": "phpstan analyse",
        "cs-fix": "php-cs-fixer fix"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\AI\\Mate\\": "src/",
            "Symfony\\AI\\Mate\\Mcp\\": "mcp/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\AI\\Mate\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        },
        "thanks": {
            "name": "symfony/ai",
            "url": "https://github.com/symfony/ai"
        },
        "symfony/ai-mate": {
            "classes": []
        }
    }
}
