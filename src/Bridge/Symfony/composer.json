{
    "name": "symfony/ai-mate-symfony",
    "description": "Symfony integration with Mate AI.",
    "license": "MIT",
    "type": "ai-mate-extension",
    "keywords": [
        "ai",
        "bridge",
        "mate"
    ],
    "authors": [
        {
            "name": "Johannes Wachter",
            "email": "johannes@sulu.io"
        },
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-simplexml": "*",
        "symfony/ai-mate": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.13"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Symfony\\AI\\Mate\\Bridge\\Symfony\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\AI\\Mate\\Bridge\\Symfony\\Tests\\": "Tests/"
        }
    },

    "config": {
        "sort-packages": true
    },
    "extra": {
        "ai-mate": {
          "scan-dirs": ["src"],
          "includes": [
            "config/services.php"
          ]
        },
        "branch-alias": {
            "dev-main": "0.x-dev"
        },
        "thanks": {
            "name": "symfony/ai",
            "url": "https://github.com/symfony/ai"
        }
    }
}
